{
    "manifest_version": 3,
    "name": "Download Manager",
    "version": "1.0",
    "description": "A cross-browser download manager extension",
    "permissions": [
      "downloads",
      "storage"
    ],
    "background": {
      "service_worker": "src/background.js"
    },
    "action": {
      "default_popup": "src/popup.html",
      "default_icon": {
        "16": "assets/icon16.png",
        "48": "assets/icon48.png",
        "128": "assets/icon128.png"
      }
    }
}
